    "require": {
        "statamic/cms": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "authors": [
        {
            "name": "Joachim RÃ¼tter"
        }
    ],
    "support": {
        "email": "me@mynetx.net"
    },
    "extra": {
        "statamic": {
            "name": "Umami Analytics",
            "description": "Add GDPR-compliant Umami analytics and see stats insights on your dashboard"
